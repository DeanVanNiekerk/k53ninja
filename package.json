{
    "name": "k53ninja",
    "version": "0.0.1",
    "private": true,
    "description": "K53 Ninja App",
    "scripts": {
        "analyze:build": "webpack --mode production --config webpack.production.js --profile --json > stats.json",
        "analyze:run": "webpack-bundle-analyzer stats.json",
        "build": "webpack --mode production --config webpack.production.js",
        "lint": "npm run lint:tsc && npm run lint:scripts",
        "lint-fix": "npm run lint-fix:script",
        "lint-fix:script": "npm run lint:script -- --fix",
        "lint:scripts": "eslint --max-warnings=10 \"src/**/*.{js,ts,tsx}\"",
        "lint:tsc": "npm run tsc",
        "start": "webpack-dev-server --mode development --config webpack.dev.js",
        "test": "jest --config=jest.config.json",
        "test:coverage": "jest --coverage --verbose --config=jest.config.json",
        "test:watch": "jest --watch --config=jest.config.json",
        "tsc": "tsc",
        "cap-copy": "cap copy",
        "svg-to-component": "npx @svgr/cli --icon"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@capacitor/android": "1.5.0",
        "@capacitor/core": "1.5.0",
