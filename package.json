        "@babel/preset-typescript": "7.8.3",
        "@capacitor/cli": "1.5.0",
        "@svgr/cli": "5.2.0",
        "@testing-library/jest-dom": "5.1.1",
        "@testing-library/react": "9.4.1",
        "@testing-library/user-event": "10.0.0",
        "@types/jest": "25.1.3",
        "@types/node": "13.7.7",
        "@types/react": "16.9.23",
