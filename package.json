        "react-redux": "7.2.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-translated": "2.5.0",
        "react-visibility-sensor": "5.1.1",
        "redux": "4.0.5",
        "redux-persist": "6.0.0",
        "redux-thunk": "2.3.0",
        "reselect": "4.0.0",
        "styled-components": "5.0.1"
    },
    "devDependencies": {
        "@babel/core": "7.8.6",
        "@babel/plugin-proposal-class-properties": "7.8.3",
